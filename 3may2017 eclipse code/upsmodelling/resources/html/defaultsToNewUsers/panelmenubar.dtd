<!ELEMENT PANELMENUBAR ( DOUBLECLICK*, MENU+ )>
<!ATTLIST PANELMENUBAR name CDATA #REQUIRED>

<!ELEMENT DOUBLECLICK EMPTY >
<!ATTLIST DOUBLECLICK name CDATA #IMPLIED >
<!ATTLIST DOUBLECLICK action_type NMTOKEN #IMPLIED >
<!ATTLIST DOUBLECLICK action_value CDATA #IMPLIED>
<!ATTLIST DOUBLECLICK action_command CDATA #IMPLIED>


<!ELEMENT MENU ( DOUBLECLICK*, MENU-ITEM* ,MENU-ITEM-GROUP*,MENU-ITEM* ) >
<!ATTLIST MENU name CDATA #REQUIRED>
<!ATTLIST MENU owner  CDATA #IMPLIED>
<!ATTLIST MENU group CDATA #IMPLIED>
<!ATTLIST MENU shortcut_key CDATA #IMPLIED>
<!ATTLIST MENU position CDATA #IMPLIED>
<!ATTLIST MENU menutype CDATA #IMPLIED>
<!ATTLIST MENU menufile CDATA #IMPLIED>

<!ELEMENT MENU-ITEM-GROUP (OPERATION*, MENU-ITEM+) >
<!ATTLIST MENU-ITEM-GROUP name CDATA #REQUIRED>

<!ELEMENT MENU-ITEM ( HTML-UI | JAVA-UI | OPERATION )* >
<!ATTLIST MENU-ITEM ACTION-ON-NO-PRIVILEGE NMTOKEN #IMPLIED >

<!ATTLIST MENU-ITEM name CDATA #REQUIRED>
<!ATTLIST MENU-ITEM action_type  CDATA #IMPLIED>
<!ATTLIST MENU-ITEM action_value CDATA #IMPLIED>
<!ATTLIST MENU-ITEM accelerator_key CDATA #IMPLIED>
<!ATTLIST MENU-ITEM accelerator_modifier CDATA #IMPLIED>
<!ATTLIST MENU-ITEM shortcut_key CDATA #IMPLIED>
<!ATTLIST MENU-ITEM action_command CDATA  #IMPLIED>

<!ELEMENT HTML-UI EMPTY>
<!ATTLIST HTML-UI action_type  CDATA #IMPLIED>
<!ATTLIST HTML-UI action_value  CDATA #IMPLIED>
<!ATTLIST HTML-UI action_command CDATA #IMPLIED>
<!ATTLIST HTML-UI action_link CDATA #IMPLIED>
<!ATTLIST HTML-UI target CDATA #IMPLIED>

<!ELEMENT JAVA-UI EMPTY>
<!ATTLIST JAVA-UI name  CDATA  #IMPLIED>
<!ATTLIST JAVA-UI action_type  CDATA  #IMPLIED>
<!ATTLIST JAVA-UI action_value  CDATA  #IMPLIED>
<!ATTLIST JAVA-UI action_command CDATA #IMPLIED>
<!ATTLIST JAVA-UI shortcut_key CDATA #IMPLIED>
<!ATTLIST JAVA-UI accelerator_modifier CDATA #IMPLIED>
<!ATTLIST JAVA-UI accelerator_key CDATA #IMPLIED>

<!ELEMENT OPERATION EMPTY>
<!ATTLIST OPERATION name CDATA #REQUIRED>

